    "lint": "eslint src/**/*.tsx"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.2.6"
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "hand-drawn",
    "organic"
  ],
  "author": "UniRoom",
  "license": "MIT"
