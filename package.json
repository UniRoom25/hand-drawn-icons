{
  "name": "@uniroom/hand-drawn-icons",
  "version": "1.0.0",
  "description": "A set of organic, hand-drawn SVG icons for React",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite website",
    "build-site": "tsc && vite build website --outDir ../dist-site",
